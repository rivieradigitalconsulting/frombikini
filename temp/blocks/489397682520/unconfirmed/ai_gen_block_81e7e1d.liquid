{% doc %}
  @prompt
    sezione a griglia in cui posso decidere distanza tra elementi della griglia, quadrati o circolari, Portiamo questi elementi a 50. Di questi elementi toglimi il contorno, si deve vedere solo immagine che deve entrare nel contenitore coprendolo tutto. Dammi anche a mobile la possibilità di visualizzare fino a 5 colonne. Poi colore di background generale. Possibilità di inserire un testo al centro del blocco, decidere font desktop e mobil e font. Poi possibilità di inserire titolo e sottotitolo sotto il blocco, decidere dimensioni per ogni device e spaziatur tra i due, fermati. I testi: centro del blocco, titolo e sottotiolo sotto non devono essere generali del blocco, bensì di ogni elemento. La sezione non deve avere titoli sottotitoli o testi globali, ma solo relativi a ogni sotto elemento
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-grid-container-{{ ai_gen_id }} {
    display: block;
    width: 100%;
    background-color: {{ block.settings.background_color }};
    padding: {{ block.settings.section_padding_top }}px 20px {{ block.settings.section_padding_bottom }}px;
  }

  .ai-grid-wrapper-{{ ai_gen_id }} {
    max-width: 1400px;
    margin: 0 auto;
  }

  .ai-grid-{{ ai_gen_id }} {
    display: grid;
    gap: {{ block.settings.grid_gap }}px;
  }

  .ai-grid-item-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    padding-bottom: 100%;
    overflow: hidden;
    {% if block.settings.item_shape == 'circle' %}
      border-radius: 50%;
    {% else %}
      border-radius: {{ block.settings.item_border_radius }}px;
    {% endif %}
  }

  .ai-grid-item-content-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    display: block;
  }

  .ai-grid-item-image-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: cover;
    display: block;
  }

  .ai-grid-item-overlay-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    text-align: center;
    padding: 10px;
    pointer-events: none;
  }

  .ai-grid-item-center-text-{{ ai_gen_id }} {
    color: {{ block.settings.center_text_color }};
    font-family: {{ block.settings.center_text_font.family }}, {{ block.settings.center_text_font.fallback_families }};
    font-weight: {{ block.settings.center_text_font.weight }};
    margin: 0;
  }

  .ai-grid-item-footer-{{ ai_gen_id }} {
    margin-top: {{ block.settings.image_text_spacing }}px;
  }

  .ai-grid-item-title-{{ ai_gen_id }} {
    font-family: {{ block.settings.title_font.family }}, {{ block.settings.title_font.fallback_families }};
    font-weight: {{ block.settings.title_font.weight }};
    color: {{ block.settings.title_color }};
    margin: 0 0 {{ block.settings.title_subtitle_spacing }}px;
  }

  .ai-grid-item-subtitle-{{ ai_gen_id }} {
    font-family: {{ block.settings.subtitle_font.family }}, {{ block.settings.subtitle_font.fallback_families }};
    font-weight: {{ block.settings.subtitle_font.weight }};
    color: {{ block.settings.subtitle_color }};
    margin: 0;
  }

  @media screen and (min-width: 750px) {
    .ai-grid-{{ ai_gen_id }} {
      grid-template-columns: repeat({{ block.settings.columns_desktop }}, 1fr);
    }

    .ai-grid-item-center-text-{{ ai_gen_id }} {
      font-size: {{ block.settings.center_text_size_desktop }}px;
    }

    .ai-grid-item-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_size_desktop }}px;
    }

    .ai-grid-item-subtitle-{{ ai_gen_id }} {
      font-size: {{ block.settings.subtitle_size_desktop }}px;
    }
  }

  @media screen and (max-width: 749px) {
    .ai-grid-{{ ai_gen_id }} {
      grid-template-columns: repeat({{ block.settings.columns_mobile }}, 1fr);
    }

    .ai-grid-item-center-text-{{ ai_gen_id }} {
      font-size: {{ block.settings.center_text_size_mobile }}px;
    }

    .ai-grid-item-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_size_mobile }}px;
    }

    .ai-grid-item-subtitle-{{ ai_gen_id }} {
      font-size: {{ block.settings.subtitle_size_mobile }}px;
    }
  }
{% endstyle %}

<grid-section-{{ ai_gen_id }}
  class="ai-grid-container-{{ ai_gen_id }}"
  {{ block.shopify_attributes }}
>
  <div class="ai-grid-wrapper-{{ ai_gen_id }}">
    <div class="ai-grid-{{ ai_gen_id }}">
      {% for i in (1..50) %}
        {% liquid
          assign image_key = 'item_' | append: i | append: '_image'
          assign link_key = 'item_' | append: i | append: '_link'
          assign center_text_key = 'item_' | append: i | append: '_center_text'
          assign title_key = 'item_' | append: i | append: '_title'
          assign subtitle_key = 'item_' | append: i | append: '_subtitle'
          
          assign item_image = block.settings[image_key]
          assign item_link = block.settings[link_key]
          assign item_center_text = block.settings[center_text_key]
          assign item_title = block.settings[title_key]
          assign item_subtitle = block.settings[subtitle_key]
        %}

        {% if item_image != blank %}
          <div>
            <div class="ai-grid-item-{{ ai_gen_id }}">
              {% if item_link != blank %}
                <a href="{{ item_link }}" class="ai-grid-item-content-{{ ai_gen_id }}">
              {% else %}
                <div class="ai-grid-item-content-{{ ai_gen_id }}">
              {% endif %}
                
                <img
                  src="{{ item_image | image_url: width: 600 }}"
                  alt="{{ item_image.alt | escape }}"
                  loading="lazy"
                  width="600"
                  height="600"
                  class="ai-grid-item-image-{{ ai_gen_id }}"
                >

                {% if item_center_text != blank %}
                  <div class="ai-grid-item-overlay-{{ ai_gen_id }}">
                    <div class="ai-grid-item-center-text-{{ ai_gen_id }}">
                      {{ item_center_text }}
                    </div>
                  </div>
                {% endif %}

              {% if item_link != blank %}
                </a>
              {% else %}
                </div>
              {% endif %}
            </div>

            {% if item_title != blank or item_subtitle != blank %}
              <div class="ai-grid-item-footer-{{ ai_gen_id }}">
                {% if item_title != blank %}
                  <h3 class="ai-grid-item-title-{{ ai_gen_id }}">{{ item_title }}</h3>
                {% endif %}
                {% if item_subtitle != blank %}
                  <div class="ai-grid-item-subtitle-{{ ai_gen_id }}">{{ item_subtitle }}</div>
                {% endif %}
              </div>
            {% endif %}
          </div>
        {% endif %}
      {% endfor %}
    </div>
  </div>
</grid-section-{{ ai_gen_id }}>

<script>
  (function() {
    class GridSection{{ai_gen_id}} extends HTMLElement {
      constructor() {
        super();
      }

      connectedCallback() {
        this.setupGrid();
      }

      setupGrid() {
        const items = this.querySelectorAll('.ai-grid-item-{{ ai_gen_id }}');
        items.forEach((item, index) => {
          item.style.animationDelay = `${index * 0.02}s`;
        });
      }
    }

    customElements.define('grid-section-{{ ai_gen_id }}', GridSection{{ai_gen_id}});
  })();
</script>

